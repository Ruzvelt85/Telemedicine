{
  "Properties": {
    "Application": "HealthCenterStructureDomainService.WebAPI"
  },
  "AllowedHosts": "*",
  "InfoLogExceptionFilterSettings": {
    "GET_api/patients/{id}_EntityNotFoundByIdException_ALL": {
      "HttpVerb": "GET",
      "Path": "api/patients/{id}",
      "Type": "Telemedicine.Common.Contracts.GlobalContracts.Exceptions.Business.EntityNotFoundByIdException"
    },
    "GET_api/users/{userId}_EntityNotFoundByIdException_ALL": {
      "HttpVerb": "GET",
      "Path": "api/users/{userId}",
      "Type": "Telemedicine.Common.Contracts.GlobalContracts.Exceptions.Business.EntityNotFoundByIdException"
    },
    "GET_api/users/{userId}/details_EntityNotFoundByIdException_ALL": {
      "HttpVerb": "GET",
      "Path": "api/users/{userId}/details",
      "Type": "Telemedicine.Common.Contracts.GlobalContracts.Exceptions.Business.EntityNotFoundByIdException"
    },
    "GET_api/Doctors/GetByInnerId/{InnerId}_EntityNotFoundByInnerIdException": {
      "HttpVerb": "GET",
      "Path": "api/Doctors/GetByInnerId/{InnerId}",
      "Type": "Telemedicine.Services.HealthCenterStructureDomainService.API.Common.DoctorsQueryService.Exceptions.EntityNotFoundByInnerIdException"
    }
  },
  "IgnoreLoggingResponseBodySettings": {
    "GET_api/patients": {
      "HttpVerb": "GET",
      "Path": "api/patients"
    }
  }
}
